{
  "name": "Potato Extension v2.0",
  "version": "1.0",
  "description": "Social streaming plugin",
  "permissions": ["declarativeContent", "storage"],  
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },  
  "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/potato_icon.png"
      }
    },
   "icons": {
      "16": "images/potato_icon.png"
    },
  "manifest_version": 2
}
